{
  "manifest_version": 3,
  "name": "Swipe Right - Auto like for Tinder",
  "description": "Auto like for Tinder using AI",
  "version": "1.1.1",
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://tinder.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tinder.com/app/recs"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
